{
    "name": "cssence",
    "displayName": "CSSence",
    "description": "This extension includes Snippets that you need to code very easily in css",
    "version": "0.0.3",
    "publisher": "PavelMelnik94",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "css",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "scss",
                "path": "./snippets/snippets.json"
            }
        ],
        "configuration": {
            "title": "CSSence",
            "properties": {
                "editor.snippetSuggestions": {
                    "type": "string",
                    "default": "top",
                    "enum": [ "top", "bottom", "inline", "none" ],
                    "description": "Controls whether snippets are shown with other suggestions and how they are sorted."
                }
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pavelmelnik94/CSSence"
    },
    "keywords": [
        "css",
        "scss",
        "snippets",
        "css snippets",
        "scss snippets"
    ],
    "author": {
        "name": "Pavel Melnik"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pavelmelnik94/CSSence/issues"
    },
    "homepage": "https://github.com/pavelmelnik94/CSSence#readme",
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#1890ff",
        "theme": "dark"
    }
}