{
    "name": "cssence",
    "displayName": "CSSence",
    "description": "This extension includes Snippets that you need to code very easily in css",
    "version": "0.0.2",
    "publisher": "PavelMelnik94",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:css",
        "onLanguage:scss"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "css",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "scss",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pavelmelnik94/CSSence"
    },
    "keywords": [
        "css",
        "scss",
        "snippets",
        "css snippets",
        "scss snippets"
    ],
    "author": {
        "name": "Pavel Melnik"
    },
    "license": "MIT"
}